{
  "name": "expressauth0",
  "version": "1.0.0",
  "description": "Express server that expose API that check ADN mutations",
  "main": "index.js",
  "scripts": {
    "dev1": "ts-node-dev --respawn --pretty --transpile-only src/index.ts",
    "start": "tsc && babel-node --extensions \".ts\" src/index.ts",
    "build": "tsc && babel src -d dist --source-maps",
    "restart": "rimraf dist && npm run build",
    "start1": "npm run build && node dist/index.js",
    "dev": "nodemon --exec npm run start1"
  },
  "author": "moviedo",
  "license": "ISC",
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/helmet": "^4.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-handlebars": "^6.0.6",
    "express-jwt": "^6.1.0",
    "helmet": "^4.6.0",
    "jwks-rsa": "^2.0.5",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/node": "^7.18.10",
    "@babel/plugin-transform-typescript": "^7.18.12",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@tsconfig/node14": "^1.0.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.13",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.7.4"
  }
}
